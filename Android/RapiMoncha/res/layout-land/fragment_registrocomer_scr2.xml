<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="2" >

    <TextView
        android:id="@+id/lbl_tituloscr2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.2"
        android:gravity="center_horizontal"
        android:text="@string/fragment_registrocomercio2_lbl_tituloscr2"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <ListView
        android:id="@+id/lstcatcomercio"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.69" >
    </ListView>

    <TextView
        android:id="@+id/lbl_tituloelementocategoria"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.1"
        android:gravity="right"
        android:text="@string/fragment_registrocomerciosky_lbl_notas"
        android:textAppearance="?android:attr/textAppearanceSmall" />

</LinearLayout>
