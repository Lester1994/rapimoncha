    <TextView
        android:id="@+id/lblnombreusuariodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/lblnombreusuario"
        android:layout_marginLeft="16dp"
        android:layout_toRightOf="@+id/lblap2usuario"
