        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lblsexousuario"
        android:layout_below="@+id/lblsexousuariodata"
        android:layout_marginTop="27dp"
        android:text="@string/activity_detalle_usuario_lblusernameusuario"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/lblusernameusuariodata"
        android:layout_width="wrap_content"
